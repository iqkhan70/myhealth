{
  "ConnectionStrings": {
    "MySQL": "server=localhost;port=3306;database=mentalhealthdb;user=YOUR_USERNAME;password=YOUR_PASSWORD"
  },
  "HuggingFace": {
    "ApiKey": "YOUR_HUGGINGFACE_API_KEY_HERE",
    "BioMistralModelUrl": "https://api-inference.huggingface.co/models/medalpaca/medalpaca-7b",
    "MeditronModelUrl": "https://api-inference.huggingface.co/models/epfl-llm/meditron-7b"
  },
  "OpenAI": {
    "ApiKey": "sk-your-actual-openai-api-key-here"
  },
  "S3": {
    "AccessKey": "YOUR_DIGITALOCEAN_ACCESS_KEY",
    "SecretKey": "YOUR_DIGITALOCEAN_SECRET_KEY",
    "BucketName": "mentalhealth-content",
    "Region": "sfo3",
    "ServiceUrl": "https://sfo3.digitaloceanspaces.com",
    "Folder": "content/"
  },
  "Jwt": {
    "Key": "YOUR_SUPER_SECRET_JWT_KEY_THAT_IS_AT_LEAST_32_CHARACTERS_LONG",
    "Issuer": "SM_MentalHealthApp",
    "Audience": "SM_MentalHealthApp_Users"
  },
  "Vonage": {
    "Enabled": true,
    "ApiKey": "YOUR_VONAGE_API_KEY",
    "ApiSecret": "YOUR_VONAGE_API_SECRET",
    "FromNumber": "+1234567890"
  },
  "Email": {
    "Enabled": true,
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587,
    "SmtpUsername": "your-email@gmail.com",
    "SmtpPassword": "your-app-password",
    "FromEmail": "your-email@gmail.com",
    "FromName": "Health App",
    "EnableSsl": true
  },
  "Agora": {
    "AppId": "YOUR_AGORA_APP_ID",
    "UseTokens": true,
    "AppCertificate": "YOUR_AGORA_APP_CERTIFICATE"
  },
  "PiiEncryption": {
    "Key": "YOUR_32_CHARACTER_ENCRYPTION_KEY_HERE"
  },
  "Encryption": {
    "Key": "YOUR_32_CHARACTER_ENCRYPTION_KEY_HERE"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5262"
      },
      "Https": {
        "Url": "https://0.0.0.0:5263",
        "Certificate": {
          "Path": "/path/to/your/certificate.pem",
          "KeyPath": "/path/to/your/certificate-key.pem"
        }
      }
    }
  }
}
