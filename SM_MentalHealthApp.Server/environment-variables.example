# DigitalOcean Spaces Credentials
# Set these environment variables before running the application
# Never commit these to version control!

export DIGITALOCEAN_ACCESS_KEY="your_access_key_here"
export DIGITALOCEAN_SECRET_KEY="your_secret_key_here"

# Mailgun Email API Configuration
# Get your API key and domain from: https://app.mailgun.com/app/dashboard
# Free tier: 5,000 emails/month for 3 months, then 1,000 emails/month
export Email__MailgunApiKey="your_mailgun_api_key_here"
export Email__MailgunDomain="your_domain.mailgun.org"

# To set these in your terminal:
# export DIGITALOCEAN_ACCESS_KEY="DO00Z6VU8Q38KXLFZ7V4"
# export DIGITALOCEAN_SECRET_KEY="ZDK61LfGdaqu5FpTcKnUfK8GNSW+cTSSbK8vK8GnMno"
